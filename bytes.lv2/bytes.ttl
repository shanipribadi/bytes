    lv2:minimum 0.0;
    lv2:maximum 1.0;
    lv2:default 0.0;
  ],
  
  
  # EG 2
  # -----------------------------------------------------------------------
  
  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 31;
    lv2:symbol "eg2attack";
    lv2:name "EG 2 Attack";
    lv2:minimum 0.0;
    lv2:maximum 1.0;
    lv2:default 0.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 32;
    lv2:symbol "eg2decay";
    lv2:name "EG 2 Decay";
    lv2:minimum 0.0;
    lv2:maximum 1.0;
    lv2:default 1.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 33;
    lv2:symbol "eg2sustain";
    lv2:name "EG 2 Sustain";
    lv2:minimum 0.0;
    lv2:maximum 1.0;
    lv2:default 0.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 34;
    lv2:symbol "eg2release";
    lv2:name "EG 2 Release";
    lv2:minimum 0.0;
