  # Modulation Methods
  # -----------------------------------------------------------------------
  
  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 35;
    lv2:symbol "method";
    lv2:name "Modulation Method";
    lv2:minimum 0.0;
    lv2:maximum 2.0;
    lv2:default 1.0;
    
    lv2:scalePoint [
      rdfs:label "Manual";
      rdf:value 0;
    ], [
      rdfs:label "EG 2";
      rdf:value 1;
    ], [
      rdfs:label "LFO";
      rdf:value 2;
    ];
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 36;
    lv2:symbol "modulation";
    lv2:name "Sync Value";
    lv2:minimum 0.0;
    lv2:maximum 1.0;
    lv2:default 0.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 37;
    lv2:symbol "rounded";
    lv2:name "Rounded";
    lv2:minimum 0.0;
    lv2:maximum 1.0;
    lv2:default 0.0;
    lv2:portProperty lv2:toggled;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 38;
    lv2:symbol "modmin";
    lv2:name "Modulation Minimum";
    lv2:minimum 0.0;
    lv2:maximum 1.0;
