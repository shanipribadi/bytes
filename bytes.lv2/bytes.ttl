    lv2:default 1.0;
  ],
  
  
  # EG 1
  # -----------------------------------------------------------------------
  
  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 27;
    lv2:symbol "eg1attack";
    lv2:name "EG 1 Attack";
    lv2:minimum 0.0;
    lv2:maximum 1.0;
    lv2:default 0.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 28;
    lv2:symbol "eg1decay";
    lv2:name "EG 1 Decay";
    lv2:minimum 0.0;
    lv2:maximum 1.0;
    lv2:default 1.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 29;
    lv2:symbol "eg1sustain";
    lv2:name "EG 1 Sustain";
    lv2:minimum 0.0;
    lv2:maximum 1.0;
    lv2:default 1.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 30;
