
@prefix atom:   <http://lv2plug.in/ns/ext/atom#>.
@prefix doap:   <http://usefulinc.com/ns/doap#>.
@prefix lv2:    <http://lv2plug.in/ns/lv2core#>.
@prefix midi:   <http://lv2plug.in/ns/ext/midi#>.
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#>.
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#>.
@prefix urid:   <http://lv2plug.in/ns/ext/urid#>.

<http://grejppi.github.io/plugins/bytes>
  a lv2:Plugin, lv2:InstrumentPlugin;
  doap:name "Bytes";
  doap:license <http://usefulinc.com/doap/licenses/gpl>;
  lv2:requiredFeature urid:map;
  
  
  # Input and output
  # -----------------------------------------------------------------------
  
  lv2:port [
    a atom:AtomPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "control";
    lv2:name "Control";
    atom:bufferType atom:Sequence;
    atom:supports midi:MidiEvent;
    lv2:designation lv2:control;
  ],
  
  [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "lout";
    lv2:name "Left Out";
  ], [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 2;
    lv2:symbol "rout";
    lv2:name "Right Out";
  ],
  
  
  # Values
  # -----------------------------------------------------------------------
  
  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 3;
    lv2:symbol "byte1";
    lv2:name "Byte 1";
    lv2:minimum 0.0;
    lv2:maximum 255.0;
    lv2:default 15.0;
    lv2:portProperty lv2:integer;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 4;
    lv2:symbol "byte2";
    lv2:name "Byte 2";
    lv2:minimum 0.0;
    lv2:maximum 255.0;
    lv2:default 15.0;
    lv2:portProperty lv2:integer;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 5;
    lv2:symbol "byte3";
    lv2:name "Byte 3";
    lv2:minimum 0.0;
    lv2:maximum 255.0;
    lv2:default 15.0;
    lv2:portProperty lv2:integer;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 6;
    lv2:symbol "byte4";
    lv2:name "Byte 4";
    lv2:minimum 0.0;
    lv2:maximum 255.0;
    lv2:default 15.0;
    lv2:portProperty lv2:integer;
  ],
  
  
  # Sync (left)
  # -----------------------------------------------------------------------
  
  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 7;
    lv2:symbol "lsync1";
    lv2:name "Byte 1 Sync (left)";
    lv2:minimum 0.5;
    lv2:maximum 8.0;
    lv2:default 1.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 8;
    lv2:symbol "lsync2";
    lv2:name "Byte 2 Sync (left)";
    lv2:minimum 0.5;
    lv2:maximum 8.0;
    lv2:default 1.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 9;
    lv2:symbol "lsync3";
    lv2:name "Byte 3 Sync (left)";
    lv2:minimum 0.5;
    lv2:maximum 8.0;
    lv2:default 1.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 10;
    lv2:symbol "lsync4";
    lv2:name "Byte 4 Sync (left)";
    lv2:minimum 0.5;
    lv2:maximum 8.0;
    lv2:default 1.0;
  ],
  
  
  # Envelope modulation of sync (left)
  # -----------------------------------------------------------------------
  
  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 11;
    lv2:symbol "lmod1";
    lv2:name "Byte 1 Mod (left)";
    lv2:minimum 0.5;
    lv2:maximum 8.0;
    lv2:default 1.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 12;
    lv2:symbol "lmod2";
    lv2:name "Byte 2 Mod (left)";
    lv2:minimum 0.5;
    lv2:maximum 8.0;
    lv2:default 1.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 13;
    lv2:symbol "lmod3";
    lv2:name "Byte 3 Mod (left)";
    lv2:minimum 0.5;
    lv2:maximum 8.0;
    lv2:default 1.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 14;
    lv2:symbol "lmod4";
    lv2:name "Byte 4 Mod (left)";
    lv2:minimum 0.5;
    lv2:maximum 8.0;
    lv2:default 1.0;
  ],
  
  
  # Sync (right)
  # -----------------------------------------------------------------------
  
  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 15;
    lv2:symbol "rsync1";
    lv2:name "Byte 1 Sync (right)";
    lv2:minimum 0.5;
    lv2:maximum 8.0;
    lv2:default 1.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 16;
    lv2:symbol "rsync2";
    lv2:name "Byte 2 Sync (right)";
    lv2:minimum 0.5;
    lv2:maximum 8.0;
    lv2:default 1.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 17;
    lv2:symbol "rsync3";
    lv2:name "Byte 3 Sync (right)";
    lv2:minimum 0.5;
    lv2:maximum 8.0;
    lv2:default 1.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 18;
    lv2:symbol "rsync4";
    lv2:name "Byte 4 Sync (right)";
    lv2:minimum 0.5;
    lv2:maximum 8.0;
    lv2:default 1.0;
  ],
  
  
  # Envelope modulation of sync (right)
  # -----------------------------------------------------------------------
  
  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 19;
    lv2:symbol "rmod1";
    lv2:name "Byte 1 Mod (right)";
    lv2:minimum 0.5;
    lv2:maximum 8.0;
    lv2:default 1.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 20;
    lv2:symbol "rmod2";
    lv2:name "Byte 2 Mod (right)";
    lv2:minimum 0.5;
    lv2:maximum 8.0;
    lv2:default 1.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 21;
    lv2:symbol "rmod3";
    lv2:name "Byte 3 Mod (right)";
    lv2:minimum 0.5;
    lv2:maximum 8.0;
    lv2:default 1.0;
  ], [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 22;
    lv2:symbol "rmod4";
    lv2:name "Byte 4 Mod (right)";
    lv2:minimum 0.5;
    lv2:maximum 8.0;
    lv2:default 1.0;
  ],
  
  
  # Gain
  # -----------------------------------------------------------------------
  
  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 23;
    lv2:symbol "gain1";
    lv2:name "Byte 1 Gain";
    lv2:minimum 0.0;
    lv2:maximum 1.0;
    lv2:default 1.0;
